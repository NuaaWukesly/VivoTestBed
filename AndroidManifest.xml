<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vivo.testbed"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="22"
        android:targetSdkVersion="22" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".BedActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".performance.ResTimeTestActivity"
            android:label="@string/title_activity_res_time_test" >
            <intent-filter>
                <action android:name="com.vivo.testbed.performance.getrestest" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".performance.multithread.ThisLockTest"
            android:label="@string/title_activity_this_lock_test" >
            <intent-filter>
                <action android:name="com.vivo.testbed.performance.multithread.thislock" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".performance.MultiThreadTest"
            android:label="@string/title_activity_mulit_thread_test" >
            <intent-filter>
                <action android:name="com.vivo.testbed.performance.multithread" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".performance.multithread.CallSameFuncTest"
            android:label="@string/title_activity_call_same_func_test" >
            <intent-filter>
                <action android:name="com.vivo.testbed.performance.multithread.callsamefunc" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RunCaseActivity"
            android:label="@string/title_activity_run_case" >
        </activity>
    </application>

</manifest>